{
    "name": "Injecta",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Injecting stuff",
    "homepage_url": "http://danharper.me",
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["inject1.js"]
        }
      ],
    "browser_action": {
      "default_title": "Inject!"
    },
    "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs",
      "activeTab"
    ]
  }